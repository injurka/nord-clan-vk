{
  "name": "nord-clan-vk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ssl": "node server.js",
    "build": "next build",
    "build:stats": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "start:ssl": "NODE_ENV=production node server.js",
    "sitemap": "next-sitemap",
    "postbuild": "next-sitemap",
    "export": "BROWSERSLIST_IGNORE_OLD_DATA=true next export",
    "lint": "pnpm lint:ts && pnpm lint:scss",
    "lint:ts": "eslint '**/*.{ts,tsx}'",
    "lint:scss": "stylelint '**/*.scss' --allow-empty-input",
    "lint:fix": "eslint --fix '**/*.{ts,tsx}' && stylelint --fix '**/*.scss'",
    "lint:staged": "lint-staged --debug",
    "format": "prettier -c \"./**/*.{ts,tsx,json,scss,md}\"",
    "format:fix": "prettier -w \"./**/*.{ts,tsx,json,scss,md}\"",
    "prepare": "husky install",
    "test": "jest -i",
    "test:watch": "jest --watch",
    "test:coverage": "jest -i --coverage",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:open": "cypress open",
    "cy:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cy:open",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cy:headless"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix --allow-empty-input"
    ],
    "*.(ts,tsx,json,scss,md)": [
      "prettier -c \"./**/*.{ts,tsx,json,scss,md}\""
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@reduxjs/toolkit": "^1.8.2",
    "@svgr/webpack": "^6.2.1",
    "axios": "^0.27.2",
    "cookies-next": "^2.0.5",
    "dayjs": "^1.11.3",
    "html-react-parser": "^2.0.0",
    "next": "12.1.6",
    "next-i18next": "^11.0.0",
    "next-pwa": "^5.5.4",
    "next-redux-wrapper": "^7.0.5",
    "next-seo": "^5.4.0",
    "next-sitemap": "^3.1.6",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.1",
    "react-redux": "^8.0.2",
    "react-transition-group": "^4.4.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.7.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@next/env": "^12.1.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^28.1.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "cypress": "^10.2.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-mixins": "^9.0.3",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4"
  },
  "packageManager": "pnpm@7.3.0"
}
